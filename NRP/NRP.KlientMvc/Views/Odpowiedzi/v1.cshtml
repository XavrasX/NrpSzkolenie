@model IEnumerable<NRP.KlientMvc.Models.v_Form_Odpowiedzi>

@{
    ViewBag.Title = "Lista odpowiedzi";
}

<h2>Lista odpowiedzi</h2>

<table class="table">
    <tr>
        <th>
           Treść pytania
        </th>
        <th>
            Odpowiedź
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>    
        <td>
            @Html.DisplayFor(modelItem => item.trescPytania)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.odpowiedz)
        </td>
    </tr>
}

</table>
